
bootargs=console=ttyS0,115200 root=/dev/mmcblk0p2 rootfstype=ext4 rootwait rw earlycon

bootcmd=fatload mmc 0:1 0x80200000 Image; fatload mmc 0:1 0x82000000 sg2002_licheervnano_sd.dtb; booti 0x80200000 - 0x82000000

bootdelay=3

kernel_addr_r=0x80200000
fdt_addr_r=0x82000000
ramdisk_addr_r=0x83000000

ethaddr=6a:04:56:fe:f9:9a
ipaddr=192.168.1.100
serverip=192.168.1.1

stderr=serial@4140000
stdin=serial@4140000
stdout=serial@4140000
